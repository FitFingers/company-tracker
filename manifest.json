{
  "name": "Company Tracker Counter",
  "description": "Count the hours worked today",
  "version": "1.0",
  "manifest_version": 3,
  "permissions": ["activeTab", "scripting"],
  "icons": {
    "16": "/images/icon.png",
    "32": "/images/icon.png",
    "48": "/images/icon.png",
    "128": "/images/icon.png"
  },

  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "/images/icon.png",
      "32": "/images/icon.png",
      "48": "/images/icon.png",
      "128": "/images/icon.png"
    }
  },

  

  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["general-functions.js"]
    }
  ]
}
